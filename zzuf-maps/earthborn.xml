<map proto="1.4.2">
<name>Earthborn</name>
<version>2.0</version>
<objective>Kill all opponent players in less than 6 minutes!</objective>
<gamemode>br</gamemode>
<created>2013-06-21</created>
<blitz/>
<rage/>
<time>6m</time>
<authors>
    <author uuid="9d4779a8-d35f-45d2-b51c-95de7a77e128"/> <!-- zzuf -->
</authors>
<contributors> 
    <contributor uuid="2e05eb91-e64b-46f8-8a49-368d205d26fd" contribution="Provided original mapmaking server"/> <!-- coastguard -->
    <contributor uuid="5364e900-6c20-4581-ba95-a102917a7132" contribution="Original map feedback"/> <!-- Allison  -->
    <contributor uuid="7955e191-1839-4c3c-ba42-2f599fe925ba" contribution="Original map feedback"/> <!-- Fairytaler -->
</contributors>
<teams>
    <team id="red-team" color="dark red" max="10">Red</team>
    <team id="yellow-team" color="yellow" max="10">Yellow</team>
</teams>
<spawns>
    <spawn team="red-team" kit="spawn-kit" yaw="180">
        <regions>
            <cuboid min="-1.5,8.5,35.5" max="1.5,8.5,37.5"/>
        </regions>
    </spawn>
    <spawn team="yellow-team" kit="spawn-kit" yaw="0">
        <regions>
            <cuboid min="-1.5,8.5,-35.5" max="1.5,8.5,-37.5"/>
        </regions>
    </spawn>
    <default>
        <regions>
            <cuboid yaw="90" min="-1.5,67.5,-1.5" max="0,67.5,1.5"/>
            <cuboid yaw="-90" min="0,67.5,-1.5" max="1.5,67.5,1.5"/>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="spawn-kit">
        <game-mode>adventure</game-mode>
        <potion duration="2" amplifier="255" ambient="true">damage resistance</potion>
        <item slot="0" unbreakable="true" name="`6`lSharpened `rGolden Carrot" lore="`7One Hit Kill" enchantment="sharpness:10" show-enchantments="false">gold sword</item>
        <item slot="1" unbreakable="true" name="`6`lPowerful `rBow" lore="`7One Hit Kill" enchantment="power:10" show-enchantments="false">bow</item>
        <item slot="2">arrow</item>
        <helmet team-color="true" locked="true">leather helmet</helmet>
    </kit>
</kits>
<timelock>on</timelock>
<kill-reward>
    <item amount="1">arrow</item>
</kill-reward>
<filters>
    <deny id="deny-players">
        <participating/>
    </deny>
    <material id="only-chest">chest</material>
</filters>
<regions>
    <above y="22" id="ceiling"/>
    <apply block="never" use="only-chest"/>
    <apply enter="never" region="ceiling"/>
</regions>
<hunger>
    <depletion>off</depletion>
</hunger>
<itemremove>
    <item>gold sword</item>
    <item>bow</item>
    <item>arrow</item>
</itemremove>
</map>
