<map proto="1.4.2">
<name>TeamKill</name>
<version>1.0</version>
<objective>Get points from eliminating other players before the time is up or everyone is dead!</objective>
<authors>
    <author uuid="dd8c3d79-3930-41f6-a815-621dc3d52649" contribution="Map Concept"/> <!-- StanleyTheCow -->
    <author uuid="9d4779a8-d35f-45d2-b51c-95de7a77e128" contribution="Map Packaging, Aesthethics and XML"/> <!-- zzuf -->
</authors>
<contributors>
    <contributor uuid="73711d58-d786-4007-8c95-c93b1311de1a" contribution="Spaceship Battles Author"/> <!-- Oversoul96 -->
</contributors>
<gamemode>blitz</gamemode>
<gamemode>ffa</gamemode>
<players min="1" max="16"/>
<time>3m</time>
 <blitz>
    <lives>3</lives>
    <broadcastLives>true</broadcastLives>
</blitz> 
<spawns>
    <spawn kit="players-kit" yaw="0" pitch="90">
        <regions>
            <point>217,120,138.5</point>
        </regions>
    </spawn>
    <default yaw="0">
        <regions>
            <cuboid min="218.5,76.5,108.5" max="215.5,76.5,105.5"/>
        </regions>
    </default>
</spawns>
<kits>
    <kit id="players-kit" force="true">
        <clear/>
        <item slot="0" amount="64" material="tnt"/>
        <item slot="1" amount="32" material="redstone block"/>
        <item slot="2" unbreakable="true" enchantment="dig speed:5" material="diamond pickaxe"/>
        <item slot="3" material="lava bucket"/>
        <item slot="4" material="water bucket"/>
        <item slot="6" unbreakable="true" material="fishing rod"/>
        <item slot="7" amount="64" material="log"/>
        <item slot="8" unbreakable="true" material="flint and steel"/>
        <helmet unbreakable="true" color="ffff55" material="leather helmet"/>
        <chestplate unbreakable="true" material="chainmail chestplate">
            <enchantment level="4">blast_protection</enchantment>
        </chestplate>
        <leggings unbreakable="true" color="ffff55" material="leather leggings"/>
        <boots unbreakable="true" color="ffff55" material="leather boots"/>
        <effect duration="oo">speed</effect>
        <effect amplifier="255" duration="5s">resistance</effect>
        <game-mode>adventure</game-mode>
    </kit>
    <kit id="survival-kit" force="true">
        <game-mode>survival</game-mode>
    </kit>
</kits>
<filters>
    <not id="no-void">
        <void/>
    </not>
    <deny id="skip-renew">
        <any>
            <material>air</material>
            <material>tnt</material>
        </any>
    </deny>
    <any id="delayed-renew">
        <material>air</material>
        <material>tnt</material>
    </any>
</filters>
<regions>
    <negative id="above-area">
        <below y="102" id="survival-area"/>
    </negative>
    <apply kit="survival-kit" region="survival-area"/>
    <apply block="never" region="above-area"/>
    <below z="113" id="obs-spawn-area"/>
    <apply enter="never" region="obs-spawn-area"/>
    <apply use="never"/>
</regions>
<timelock>on</timelock>
<disabledamage>
    <damage>entity attack</damage>
</disabledamage>
<hunger>
    <depletion>off</depletion>
</hunger>
<maxbuildheight>70</maxbuildheight>
<score>
    <kills>1</kills>
</score>
<tnt>
    <fuse>3s</fuse>
</tnt>
<renewables>
    <renewable interval="25s" renew-filter="skip-renew" avoid-players="0"/>
    <renewable interval="40s" renew-filter="delayed-renew" avoid-players="0" grow="true"/>
</renewables>
<gamerules>
    <doTileDrops>false</doTileDrops>
    <naturalRegeneration>false</naturalRegeneration>
    <doFireTick>false</doFireTick>
</gamerules>
<itemremove>
    <item>leather helmet</item>
    <item>chainmail chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
</itemremove>
</map>
